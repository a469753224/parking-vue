.search-warp {
  position: fixed;
  left: 0;
  top: 0;
  width: 100vw;
  height: 88rpx;
  z-index: 7777;
  box-shadow: 0 5rpx 5rpx 0 rgba(0,0,0,.05);
}

.map {
  width: 100vw;
  height: 50vh;
}

.map map {
  width: 100vw;
  height: 50vh;
}

.search-list {
  width: 100vw;
  height: 45vh;
  overflow-y: auto;
  padding: 24rpx 0 0;
}

.cell {
  display: flex;
  justify-items: flex-start;
  width: 100vw;
  min-height: 114rpx;
  padding:  0 24rpx 0;
  box-sizing: border-box;
  margin: 20rpx 0 0;
}

.icon-warp {
  display: flex;
  justify-items: center;
  justify-content: center;
  width: 20rpx;
  height: 40rpx;
  margin: 10rpx 20rpx 0 0;
}

.position-icon {
  width: 20rpx;
  height: 24rpx;
  object-fit: cover;
  flex-shrink: 0;
}

.history-icon {
  width: 24rpx;
  height: 24rpx;
  object-fit: cover;
  flex-shrink: 0;
}

.position-lately {
  width: 24rpx;
  height: 24rpx;
  object-fit: cover;
  flex-shrink: 0;
}

.used-address {
  width: 40rpx;
  height: 39rpx;
  flex-shrink: 0;
}

.address {
  width: 100%;
  min-height: 114rpx;
}

.address-title {
  display: flex;
  justify-items: center;
  font-size: 30rpx;
}

.bod {
  font-weight: 700;
}

.address-detail {
  display: flex;
  justify-items: center;
  justify-content: space-between;
  width: 100%;
  padding: 15rpx 0 24rpx;
  border-bottom: 1rpx solid rgba(242, 242, 242, 1);
}

.address-desc {
  font-size: 24rpx;
  color: rgba(153, 153, 153, 1);
}

.address-distance {
  flex-shrink: 0;
  font-size: 24rpx;
  color: rgba(153, 153, 153, 1);
}

.used-cell {
  width: 100vw;
  height: 148rpx;
  display: flex;
  align-items: flex-start;
  padding: 39rpx 24rpx;
  box-sizing: border-box;
}

.used-address-icon {
  width: 40rpx;
  height: 39rpx;
  flex-shrink: 0;
  margin: 0 -20rpx 0 0;
}

.used-addr {
  font-size: 32rpx;
  font-weight: 700;
  margin: 0 0 15rpx;
  padding: 0 0 0 20rpx;
  color: rgba(51, 51, 51, 1);
}

.used {
  display: flex;
  flex-direction: column;
  width: 100%;
  margin: 0 0 0 20rpx;
  padding: 0 0 24rpx;
  border-bottom: 1rpx solid rgba(242, 242, 242, 1);
}

.used-tag {
  font-size: 24rpx;
  padding: 0 0 0 20rpx;
  color: rgba(153, 153, 153, 1);
}


