page {
  background-color: rgba(245, 245, 245, 1);
}

.card {
  width: 702rpx;
  height: auto;
  margin: 24rpx 24rpx 0;
  border-radius: 20rpx;
  background-color: rgb(255, 255, 255);
}

.status-bar {
  display: flex;
  align-items: center;
  width: 100%;
  height: 96rpx;
  border-radius: 20rpx 20rpx 0 0;
  background-color: rgba(255, 201, 26, 1);
}

.status-bar text {
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 26rpx;
  color: rgba(255, 179, 26, 1);
  padding: 10rpx 24rpx;
  border-radius: 0 28rpx 28rpx 0;
  background-color: rgba(255, 255, 255, 1);
}

.item-row {
  display: flex;
  align-items: center;
  padding: 18rpx 24rpx;
}

.row {
  display: flex;
  flex-direction: column;
  width: 50%;
}

.label {
  font-size: 26rpx;
  color: rgba(153, 153, 153, 1);
  margin: 0 0 8rpx;
}

.flie {
  font-size: 30rpx;
  color: rgba(51, 51, 51, 1);
}

.address {
  display: flex;
  flex-direction: column;
  padding: 18rpx 24rpx 64rpx;
}


.tips {
  padding: 0 24rpx;
  font-size: 26rpx;
  margin: 0 0 72rpx;
  color: rgba(255, 83, 26, 1);
}

.toolbar {
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 0 24rpx 42rpx;
}

.date {
  font-size: 26rpx;
  color: rgba(153, 153, 153, 1);
}

.btn {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 210rpx;
  height: 64rpx;
  font-size: 26rpx;
  color: rgba(51, 51, 51, 1);
  border-radius: 32rpx;
  border: 2rpx solid rgba(230, 230, 230, 1);
}

.reapply {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 210rpx;
  height: 64rpx;
  font-size: 26rpx;
  border-radius: 32rpx;
  color: rgba(43, 34, 24, 1);
  background-color: rgba(255, 201, 26, 1);
}

.dalate {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 128rpx;
  height: 64rpx;
  font-size: 26rpx;
  color: rgba(51, 51, 51, 1);
  border-radius: 32rpx;
  border: 2rpx solid rgba(230, 230, 230, 1);
}