.card {
  display: flex;
  justify-content: space-between;
  width: 702rpx;
  height: 234rpx;
  padding: 40rpx 32rpx;
  margin: 0 auto;
  box-sizing: border-box;
}

.title-warp {
  display: flex;
  flex-direction: column;
  font-size: 26rpx;
  color: rgba(242, 207, 157, 1);
}

.title {
  font-size: 34rpx;
  margin: 0 0 8rpx;
}

.term {
  margin: 16rpx 0 0;
  font-size: 26rpx;
  color: rgb(255,255,255);
}

.btn {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 144rpx;
  height: 56rpx;
  font-size: 26rpx;
  border-radius: 28rpx;
  margin: 50rpx 0 0;
  color: rgba(71, 71, 71, 1);
  background-color: rgb(255, 255, 255);
}

.single {
  display: flex;
  flex-direction: column;
  justify-content: center;
  height: 144rpx;
  font-size: 26rpx;
  color: rgba(242, 207, 157, 1);
}

.money text:nth-child(1){
  font-size: 34rpx;
  margin: 0 8rpx 0 0;
}















